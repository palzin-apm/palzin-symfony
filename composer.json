{
    "name": "palzin-apm/palzin-symfony",
    "description": "Real-Time monitoring Symfony package using Palzin Monitor (APM)",
    "keywords": ["monitoring", "php", "palzin", "symfony", "bundle"],
    "homepage": "https://palzin.app",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Palzin Team",
            "email": "hello@palzin.app"
        }
    ],
    "require": {
		"php": ">=7.2.5",
		"palzin-apm/palzin-php": "^3.6",
        "symfony/config": "^4.4|^5.2|^6.0",
        "symfony/dependency-injection": "^4.4|^5.2|^6.0",
        "symfony/http-kernel": "^4.4|^5.2|^6.0",
        "symfony/event-dispatcher": "^4.4|^5.2|^6.0",
        "symfony/console": "^4.4|^5.2|^6.0",
        "symfony/framework-bundle": "^4.4|^5.2|^6.0",
        "symfony/yaml": "^4.4|^5.2|^6.0",
        "symfony/security-bundle": "^4.4|^5.2|^6.0"
	},
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.4",
        "symfony/framework-bundle": "^4.4|^5.2|^6.0",
        "phpunit/phpunit": "^9.5",
        "symfony/test-pack": "^1.0",
        "symfony/messenger": "^4.4|^5.2|^6.0"
    },
	"autoload": {
        "psr-4": {
            "Palzin\\Symfony\\Bundle\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Palzin\\Symfony\\Bundle\\Tests\\": "tests/"
        }
    },
	"minimum-stability": "dev",
    "prefer-stable": true
}
